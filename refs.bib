  author = {Yang, ZX and Xia, YD and Mokaya, R},
  month = nov,
  year = {2005},
  keywords = {N-doped, graphitic carbon, hollow shells, porous carbon, templating, zeolite},
  pages = {69--80},
}

@article{yang_aligned_2005,
