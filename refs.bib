@inproceedings{sugimura_electron_2000,
  address = {184 THORN HILL RD, WARRENDALE, PA 15086-7514 USA},
  title = {Electron field emission from nitrogen-doped amorphous carobn thin films prepared by arc ion plating},
  isbn = {0-87339-495-X},
  abstract = {Amorphous carbon thin films doped with nitrogen (a-C:N) have been synthesized by means of shielded are ion plating (SAIP). Their electron field emission properties, that is, threshold field strengths for electron emission and maximum emission current densities, were evaluated and compared with those of pure amorphous carbon (a-C) films prepared by the same SAIP apparatus. Among all of the fabricated a-C:N films, the a-C:N films with a N concentration of 23 \%, which prepared using N-2 arc plasm at a pressure of 1 Pa with applying a sample bias voltage of -100 V, showed the lowest threshold filed of 13.4 V/mum and the highest emission current density of 1.87 muA/cm(2) at a field of 20 V/mum. These field emission properties of the a-C:N film were much better than those of the a-C films. The best results obtained on the a-C film in this study were the lowest threshold filed of 15.5 V/mum and the highest emission current density of 0.052 muA/cm(2) at a field of 20 V/mum. Nitrogen doping to a-C was found to be effective for lowering threshold voltage for field-emission and for increasing emission current density. The electron field emission characteristics of the a-C:N films depended not only on N concentration but also on chemical bonding state of nitrogen in the a-C:N films. The presence of the beta -C3N4-like phase, which contributes to improve wear resistance of a-C:N as well, distinguished by the N1s XPS peak at 400.5 eV is proved to be crucial in order to enhance field emission properties of a-C:N.},
  language = {English},
  booktitle = {{SECOND} {INTERNATIONAL} {CONFERENCE} {ON} {PROCESSING} {MATERIALS} {FOR} {PROPERTIES}},
  publisher = {MINERALS, METALS \& MATERIALS SOC},
  author = {Sugimura, H and Sato, Y and Tajima, N and Takai, O},
  editor = {Mishra, B and Yamauchi, C},
  year = {2000},
  pages = {315--318},
}

@article{suffredini_enhanced_2007,
