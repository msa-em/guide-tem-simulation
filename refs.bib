@article{he_extended_2014,
  title = {Extended {Klein} {Edges} in {Graphene}},
  volume = {8},
  issn = {1936-0851},
  url = {https://doi.org/10.1021/nn504471m},
  doi = {10.1021/nn504471m},
  abstract = {Graphene has three experimentally confirmed periodic edge terminations, zigzag, reconstructed 5–7, and arm-chair. Theory predicts a fourth periodic edge of graphene called the extended Klein (EK) edge, which consists of a series of single C atoms protruding from a zigzag edge. Here, we confirm the existence of EK edges in both graphene nanoribbons and on the edge of bulk graphene using atomic resolution imaging by aberration-corrected transmission electron microscopy. The formation of the EK edge stems from sputtering and reconstruction of the zigzag edge. Density functional theory reveals minimal energy for EK edge reconstruction and bond distortion both in and out of plane, supporting our TEM observations. The EK edge can now be included as the fourth member of observed periodic edge structures in graphene.},
  number = {12},
  journal = {ACS Nano},
  author = {He, Kuang and Robertson, Alex. W. and Lee, Sungwoo and Yoon, Euijoon and Lee, Gun-Do and Warner, Jamie H.},
  month = dec,
