@article{huang_crystalline_2008,
  title = {Crystalline nanowires of {Ln}(2){O}(2){S}, {Ln}(2){O}(2){S}(2), {LnS}(2) ({Ln} = la, nd), and {La2O2S} : {EU3}+. {Conversions} via the boron-sulfur method that preserve shape},
