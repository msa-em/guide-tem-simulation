  journal = {Nano Lett.},
  author = {Li, Yiming and Mann, David and Rolandi, Marco and Kim, Woong and Ural, Ant and Hung, Steven and Javey, Ali and Cao, Jien and Wang, Dunwei and Yenilmez, Erhan and Wang, Qian and Gibbons, James F. and Nishi, Yoshio and Dai, Hongjie},
