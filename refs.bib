@article{likodimos_electronic_2005,
  title = {Electronic properties of boron-doped multiwall carbon nanotubes studied by {ESR} and static magnetization},
  volume = {72},
  issn = {1098-0121},
  abstract = {The electronic properties of boron-doped multiwall carbon nanotubes (MWNTs) have been studied using static magnetization and electron spin resonance. A relatively strong ferromagnetic signal has been identified in the dc magnetization response that shows the occurrence of ferromagnetism, coexisting with the orbital and spin magnetism of the conduction electrons. The small diamagnetic susceptibility and the weak temperature variation of the g factor, which is observed in the narrow conduction-electron spin resonance (CESR), indicate a Fermi level shift of similar to 0.2 eV, according to the quasi-two-dimensional graphite band model. The temperature dependence of the spin susceptibility reveals considerable enhancement compared to undoped MWNTs and the presence of thermally activated behavior, complying with an increased density of states and the formation of localized impurity states close to the Fermi level. Line-shape analysis of the CESR spectra in terms of two separate spin systems implies enhanced doping inhomogeneity.},
  language = {English},
  number = {4},
  journal = {Phys. Rev. B},
  author = {Likodimos, V and Glenis, S and Lin, CL},
  month = jul,
  year = {2005},
}

