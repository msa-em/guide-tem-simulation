@article{larciprete_dual_2011,
  title = {Dual {Path} {Mechanism} in the {Thermal} {Reduction} of {Graphene} {Oxide}},
  volume = {133},
  abstract = {Graphene is easily produced by thermally reducing graphene oxide. However, defect formation in the C network during deoxygenation compromises the charge carrier mobility in the reduced material. Understanding the mechanisms of the thermal reactions is essential for defining alternative routes able to limit the density of defects generated by carbon evolution. Here, we identify a dual path mechanism in the thermal reduction of graphene oxide driven by the oxygen coverage: at low surface density, the O atoms adsorbed as epoxy groups evolve as O2 leaving the C network unmodified. At higher coverage, the formation of other O-containing species opens competing reaction channels, which consume the C backbone. We combined spectroscopic tools and ab initio calculations to probe the species residing on the surface and those released in the gas phase during heating and to identify reaction pathways and rate-limiting steps. Our results illuminate the current puzzling scenario of the low temperature gasification of graphene oxide.},
  number = {43},
  journal = {J. Am. Chem. Soc.},
  author = {Larciprete, Rosanna and Fabris, Stefano and Sun, Tao and Lacovig, Paolo and Baraldi, Alessandro and Lizzit, Silvano},
