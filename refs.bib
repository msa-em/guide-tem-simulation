  booktitle = {{PROCEEDINGS} {OF} {THE} {6TH} {INTERNATIONAL} {CONFERENCE} {ON} {FUEL} {CELL} {SCIENCE}, {ENGINEERING}, {AND} {TECHNOLOGY} - 2008},
