@article{kretschmer_formation_2020,
  title = {Formation of {Defects} in {Two}-{Dimensional} {MoS2} in the {Transmission} {Electron} {Microscope} at {Electron} {Energies} below the {Knock}-on {Threshold}: {The} {Role} of {Electronic} {Excitations}},
