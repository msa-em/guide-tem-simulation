  journal = {Chem. Mater.},
  author = {Deng, Dehui and Pan, Xiulian and Yu, Liang and Cui, Yi and Jiang, Yeping and Qi, Jing and Li, Wei-Xue and Fu, Qiang and Ma, Xucun and Xue, Qikun and Sun, Gongquan and Bao, Xinhe},
