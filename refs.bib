  journal = {Nano Lett.},
  author = {Choi, YM and Lee, DS and Czerw, R and Chiu, PW and Grobert, N and Terrones, M and Reyes-Reyes, M and Terrones, H and Charlier, JC and Ajayan, PM and Roth, S and Carroll, DL and Park, YW},
