@article{farahani_density_2009,
  title = {Density {Functional} {Study} of {Al}-27 and {N}-14 {Quadrupole} {Coupling} {Constants} in the {Carbon} {Doped} (4,4) {Single}-{Walled} {Armchair} {Aluminum} {Nitride} {Nanotube}},
